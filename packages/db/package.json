    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "next-auth": "^4.20.1"
  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "prisma": "^4.9.0",
    "typescript": "^4.9.4"
  }
}
